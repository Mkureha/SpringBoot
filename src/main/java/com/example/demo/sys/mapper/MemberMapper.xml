<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.sys.mapper.MemberMapper">

	<select id="MemberLogin" parameterType="string"
		resultType="com.example.demo.sys.domain.MemberVO">
		SELECT *
		FROM member
			WHERE user_name != #{user_name} OR password != #{password}
	</select>
</mapper>